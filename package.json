{
  "name": "sketchxport-plugin",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketchxport-plugin",
    "manifest": "src/manifest.json",
    "main": "sketchxport-plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link",
    "lint": "itp-react-scripts lint",
    "lint:fix": "npm run lint -- --fix",
    "format": "itp-react-scripts format src/*/** --write",
    "test": "skpm-test"
  },
  "devDependencies": {
    "@inthepocket/itp-react-scripts": "^0.47.1",
    "@skpm/builder": "^0.5.2",
    "@skpm/test-runner": "^0.3.3",
    "eslint": "^5.7.0"
  },
  "author": "Jonathan Dierckens <jonathan.dierckens@inthepocket.mobi>",
  "dependencies": {
    "@skpm/child_process": "^0.2.4",
    "camelcase": "^5.0.0",
    "color-convert": "^1.9.3",
    "color-namer": "^1.3.0",
    "sketch": ".",
    "sketch-utils": "^0.2.8"
  }
}

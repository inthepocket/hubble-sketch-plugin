{
  "name": "itp-sketchxport-plugin",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "itp-sketchxport-plugin",
    "manifest": "src/manifest.json",
    "main": "itp-sketchxport-plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link",
    "lint": "itp-react-scripts lint",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "@inthepocket/itp-react-scripts": "^0.47.1",
    "@skpm/builder": "^0.5.2",
    "eslint": "^5.7.0"
  },
  "author": "Jonathan Dierckens <jonathan.dierckens@inthepocket.mobi>"
}

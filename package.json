{
  "name": "hubble-sketch-plugin",
  "description": "A Sketch plugin to extract assets and primitives and send them to Hubble.app",
  "version": "0.0.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "hubble-sketch-plugin",
    "manifest": "src/manifest.json",
    "main": "hubble-sketch-plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inthepocket/hubble-sketch-plugin"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link",
    "lint": "itp-react-scripts lint",
    "lint:fix": "npm run lint -- --fix",
    "format": "itp-react-scripts format \"src/**/*.js\" --write",
    "test": "skpm-test",
    "log": "skpm log -f",
    "release:patch": "skpm publish patch",
    "release:minor": "skpm publish minor",
    "release:major": "skpm publish major"
  },
  "devDependencies": {
    "@inthepocket/itp-react-scripts": "^0.47.1",
    "@skpm/builder": "^0.5.2",
    "@skpm/test-runner": "^0.3.3",
    "eslint": "^5.7.0"
  },
  "contributors": [
    "Jonathan Dierckens <jonathan.dierckens@inthepocket.mobi>",
    "Thibault Maekelbergh <thibault.maekelbergh@icloud.com>"
  ],
  "dependencies": {
    "@skpm/child_process": "^0.2.4",
    "sketch": ".",
    "sketch-utils": "^0.2.8"
  }
}
